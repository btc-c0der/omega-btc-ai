[run]
source = omega_ai
omit = 
    */tests/*
    */__init__.py
    */migrations/*
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory = htmlcov
title = OMEGA BTC AI Divine Coverage Report