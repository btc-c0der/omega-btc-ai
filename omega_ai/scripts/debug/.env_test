# BitGet API Credentials - TESTNET
# Replace these values with your own API credentials
BITGET_TESTNET_API_KEY=TEST_API_KEY
BITGET_TESTNET_SECRET_KEY=TEST_SECRET_KEY  
BITGET_TESTNET_PASSPHRASE=TEST_PASSPHRASE

# BitGet API Credentials - MAINNET
# Replace these values with your own API credentials
BITGET_API_KEY=your_mainnet_api_key_here
BITGET_SECRET_KEY=your_mainnet_secret_key_here
BITGET_PASSPHRASE=your_mainnet_passphrase_here

# Sub-account settings
# You can use either STRATEGIC_SUB_ACCOUNT_NAME (for strategic trader) 
# or the generic SUB_ACCOUNT_NAME
STRATEGIC_SUB_ACCOUNT_NAME=TEST_SUB_ACCOUNT
SUB_ACCOUNT_NAME=your_sub_account_name_here

# Debug settings
# Set to "true" to enable detailed debugging
BITGET_DEBUG=false 